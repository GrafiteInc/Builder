{
    "name": "yab/laracogs",
    "description": "Prepare your Laravel apps incredibly fast, with various commands, services, facades and boilerplates.",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Lantz",
            "email": "matt@yabhq.com"
        },
        {
            "name": "Chris Blackwell",
            "email": "chris@yabhq.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "doctrine/dbal": "^2.5",
        "illuminate/support": "^5.6",
        "yab/crudmaker": "^1.3",
        "yab/formmaker": "^1.2",
        "yab/crypto": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "~",
        "mikey179/vfsStream": "^1.6",
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Yab\\Laracogs\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yab\\Laracogs\\LaracogsProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
